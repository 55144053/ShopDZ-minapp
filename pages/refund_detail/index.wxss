page {
  background: rgb(238, 237, 237);
}
.myorder2 {
    width: 90%;
    padding: 0 5%;
    margin-bottom: 25rpx;
    background: #fff;
}
.refund-product-tit {
    font-size: 30rpx;
    border-bottom: 1px solid #f5f5f5;
    line-height: 3;
    font-weight: normal;
}
.myorder2-det {
    font-size: 29rpx;
    line-height: 1.8;
    overflow: hidden;
    zoom: 1;
    padding: .19rpx 0;
    border-bottom: 1px solid #f0f0f0;
}
.left {
    float: left;
}

.myorder2-det-span {
    width: 23%;
    text-align: center;
}
.myorder2-det-p {
    width: 77%;
    text-align: left;
}
.refund-img-list {
    width: 77%;
}
.con-img-list {
    width: 100%;
    overflow: hidden;
    zoom: 1;
    margin-top: 25rpx;
}
.refund-img-list .li {
    width: 30%;
    list-style: none;
    margin-right: 3%;
}
.con-img-list .li {
    float: left;
    width: 24%;
    margin-right: 1%;
    overflow: hidden;
}
.con-img-list li>img {
    display: inline-block;
    width: 100%;
    /* height: 100%; */
    cursor: pointer;
    vertical-align: middle;
}


.center_item{
  overflow: hidden;
  padding: 24rpx 0;
  border-bottom: 1rpx solid #F0F0F0;
}
.center_item:last-child{
  border-bottom: 0;
}
.center_img{
  width: 140rpx;
  height: 140rpx;
  overflow: hidden;
}
.center_img image{
  width: 100%;
  height: 100%;
}
.guige{
  width: 518rpx;
  padding-top: 20rpx;
  font-size: 26rpx;
}
.guige_bar{
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden
}
.footer_{
  padding:  24rpx 0;
  border-bottom: 1rpx solid #F0F0F0;
}
.footer_:last-child{
  border-bottom: 0;
}
.footer_bar{
  font-size: 26rpx;
  overflow: hidden;
}
.footer_bar .fl{
  line-height: 46rpx;
}